{
  "name": "discovery",
  "version": "0.1.9",
  "description": "a network security analysis and visualization tool",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development electron .",
    "lint": "prettier --no-semi --single-quote --write './app/src/**/*.js' && standard './app/src/**/*.js' --fix",
    "postinstall": "electron-builder install-app-deps",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyberLens/Discovery"
  },
  "keywords": [
    "Electron",
    "network",
    "security"
  ],
  "author": "or3stis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CyberLens/Discovery/issues"
  },
  "homepage": "https://github.com/CyberLens/Discovery/#readme",
  "engines": {
    "node": ">=10.6.0"
  },
  "build": {
    "productName": "Discovery",
    "appId": "org.or3stis.Discovery",
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "build/icon.png"
    }
  },
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "cytoscape": "^3.16.2",
    "jsonfile": "^6.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^15.5.5",
    "electron-builder": "^22.8.1",
    "prettier": "^2.1.2",
    "standard": "^16.0.3"
  }
}
